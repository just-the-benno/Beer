{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppConfiguration": {
    "OpenIdConnection": {
      "Authority": "https://localhost:5001",
      "ClientId": "beershark",
      "RedirectUri": "https://localhost:5301/authentication/login-callback",
      "PostLogoutRedirectUri": "https://localhost:5301/authentication/logout-callback",
      "ResponseType": "code",
      "Scopes": [ "openid", "profile" ]
    },
    "APIUrls": {
      "MyAPI": "https://localhost:5001/"
    },
    "AppUrls": {
      "ControlCenter": "https://localhost:5101/"
    }
  }
}
