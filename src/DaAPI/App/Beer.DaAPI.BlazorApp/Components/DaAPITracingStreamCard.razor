<MudCard Class="mt-5">
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h6">Tracing</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
        <DaAPITracingStream 
            ShowProcedureIdentifier="@ShowProcedureIdentifier" 
            ShowModuleIdentifier="@ShowModuleIdentifier" 
            ModuleIdentifier="@ModuleIdentifier" 
            ProcedureIdentiifier="@ProcedureIdentiifier"
            EntityId="@EntityId" />
    </MudCardContent>
</MudCard>

@code {

        [Parameter] public Int32? ModuleIdentifier { get; set; }
        [Parameter] public Int32? ProcedureIdentiifier { get; set; }
        [Parameter] public Guid? EntityId { get; set; }
        [Parameter] public Boolean ShowModuleIdentifier { get; set; }
        [Parameter] public Boolean ShowProcedureIdentifier { get; set; }
}
